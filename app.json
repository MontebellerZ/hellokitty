{
  "expo": {
    "name": "Hello Kitty",
    "slug": "hellokitty",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/icon/icon.png",
    "scheme": "hellokitty",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/icon.png",
        "backgroundColor": "#f7c1ea"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.montebellerz.hellokitty"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f7c1ea",
          "image": "./assets/icon/icon.png",
          "resizeMode": "cover",
          "imageWidth": 100
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir acesso à câmera",
          "microphonePermission": "Permitir acesso ao microfone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir acesso às fotos",
          "savePhotosPermission": "Permitir salvar fotos",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ca163339-b908-489d-8bfc-0d918e371893"
      }
    }
  }
}